<div class="container">
<div class="contents center row" >
  <p><%= @nickname %>さんの投稿一覧</p>
  <% @blogs.each do |blog| %>
    <div class="row">
      <div class="col s12 m12">
        <div class="card white">
          <div class="card-content black-text">
            <span class="card-title"></span>

              <p><%= render partial: "blogs/blog", locals: { blog: blog } %></p>
          </div>
        </div>
      </div>
    </div>
  <% end %>
  <%= paginate(@blogs) %>
</div>
</div>
